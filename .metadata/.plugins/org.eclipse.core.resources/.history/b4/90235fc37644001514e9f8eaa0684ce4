
public class Methods {
public static void main(String[]args){
	for(int i = 1; i<=10000; i++){
	System.out.println(i);
}
	numbers (10,9000);
}

static void numbers(int start, int end){
	if (start<end)
	for(int i = start; i<= end; i++){
		System.out.println(i);
	}
	else
	{
for(int i = start; i >= end; i--){
	System.out.println(i);
	}
}
}

static void exponents(int base, int exp)
{  int ans =1;
}
{
for(int i = 1; i<= exp; i++)
{
ans*=base;
}
{
return ans;

}
